{
    "build": {
        "env": {
            "GO_BUILD_FLAGS": "-ldflags '-s -w'"
        }
    },
    "routes": [
        {
            "src": "/_next/(.*)",
            "dest": "/_next/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/short.go"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}